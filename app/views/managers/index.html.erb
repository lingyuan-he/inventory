<h1>List all Managers</h1>
<table border="1">
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Phone</th>
    <th>Email</th>
    <th colspan = "3"> </th>
  </tr>
 
  <% @managers.each do |manager| %>
    <tr>
      <td><%= manager.id %></td>
      <td><%= manager.name %></td>
      <td><%= manager.phone %></td>
      <td><%= manager.email %></td>
      <td><%= link_to 'Details', manager_path(manager) %></td>
      <td><%= link_to 'Edit', edit_manager_path(manager) %></td>
      <td><%= link_to 'Destroy', manager_path(manager), :method => :delete, :confirm => 'Are you sure?' %></td>
    </tr>
  <% end %>
</table>
